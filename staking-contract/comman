mxpy --verbose contract deploy --bytecode=/Users/jhonattan/Documents/chaallenger/staking-contract/output/staking-contract.wasm \
    --recall-nonce --pem=/Users/jhonattan/Documents/chaallenger/staking-contract/wallet-owner.pem \
    --gas-limit=60000000 \
    --send --outfile="deploy-devnet.interaction.json" --wait-result \
    --proxy=https://devnet-gateway.multiversx.com --chain=D

